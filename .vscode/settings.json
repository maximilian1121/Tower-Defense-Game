{
    "typescript.tsdk": "node_modules/typescript/lib",
    "files.eol": "\n",
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.formatOnSave": true
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.formatOnSave": true
    },
    "eslint.run": "onType",
    "eslint.format.enable": true,
    "eslint.useFlatConfig": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/flutter/ephemeral": true,
        "**/Flutter/ephemeral": true,
        "**/.symlinks": true,
        "**/.plugin_symlinks": true,
        "node_modules": true,
        "include": true
    },
    "hide-files.files": ["node_modules", "include"],
    "cSpell.words": [
        "builderman",
        "gsub",
        "Hotbar",
        "imagebutton",
        "imagelabel",
        "Keypoint",
        "Leaderstats",
        "Lerp",
        "levelbar",
        "Managment",
        "rbxasset",
        "rbxassetid",
        "screengui",
        "Stackable",
        "uiaspectratioconstraint",
        "uicorner",
        "uiflexitem",
        "uigradient",
        "uilistlayout",
        "uipadding",
        "uistroke",
        "uitextsizeconstraint",
        "viewportframe"
    ],
    "editor.formatOnType": true,
    "luau-lsp.plugin.enabled": true,
    "typescript.validate.enable": true,
    "typescript.tsserver.experimental.enableProjectDiagnostics": true,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/.dart_tool": true,
        "**/out": true
    }
}
